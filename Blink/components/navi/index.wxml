<!--components/navi/index.wxml-->
<view class="navi-container">
	<image bind:tap="onLeft" class="icon" src="{{latest ? disLeftSrc : leftSrc}}" mode="widthFix"></image>
	<text class="title">{{title}}</text>
	<image bind:tap="onRight" class="icon" src="{{first ? disRightSrc : rightSrc}}" mode="widthFix"></image>
</view>
